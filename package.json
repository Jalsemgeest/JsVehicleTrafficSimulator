    "test": "tests/allTests.html"
  },
  "scripts": {
    "test": "grunt --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bicarbon8/JsVehicleTrafficSimulator.git"
  },
  "keywords": [
    "javascript",
